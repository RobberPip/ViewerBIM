{
	"name": "frontend",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc -b && vite build",
		"lint": "biome check --apply ./frontend/src",
		"preview": "vite preview",
		"openapi": "npm run openapi:types && npm run openapi:zod",
		"openapi:types": "openapi-typescript http://127.0.0.1:5018/openapi.json -o ./src/shared/api/schema-ai.d.ts && biome format --write ./src/shared/api/schema-ai.d.ts",
		"openapi:zod": "typed-openapi http://127.0.0.1:5018/openapi.json -o ./src/shared/api/zod-ai.ts -r zod && biome format --write ./src/shared/api/zod-ai.ts",
		"openapi-auth": "npm run openapi-auth:types && npm run openapi-auth:zod",
		"openapi-auth:types": "openapi-typescript http://127.0.0.1:5000/swagger/v1/swagger.json -o ./src/shared/api/schema-auth.d.ts && biome format --write ./src/shared/api/schema-auth.d.ts",
		"openapi-auth:zod": "typed-openapi http://127.0.0.1:5000/swagger/v1/swagger.json -o ./src/shared/api/zod-auth.ts -r zod && biome format --write ./src/shared/api/zod-auth.ts",
		"openapi-proj:types": "openapi-typescript http://127.0.0.1:5005/swagger/v1/swagger.json -o ./src/shared/api/schema-projects.d.ts && biome format --write ./src/shared/api/schema-projects.d.ts",
		"openapi-proj:zod": "typed-openapi http://127.0.0.1:5005/swagger/v1/swagger.json -o ./src/shared/api/zod-projects.ts -r zod && biome format --write ./src/shared/api/zod-projects.ts"
	},
	"dependencies": {
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.0",
		"@farfetched/atomic-router": "^0.12.8",
		"@farfetched/core": "^0.12.8",
		"@mui/icons-material": "^7.1.0",
		"@mui/lab": "^7.0.0-beta.12",
		"@mui/material": "^7.1.0",
		"@mui/x-data-grid": "^7.28.1",
		"@mui/x-date-pickers": "^7.28.3",
		"@tailwindcss/vite": "^4.0.15",
		"@tanstack/react-form": "^1.1.2",
		"@tanstack/zod-form-adapter": "^0.42.1",
		"@thatopen/components": "^2.4.3",
		"@thatopen/components-front": "^2.4.3",
		"@thatopen/fragments": "^2.4.0",
		"@thatopen/ui": "^2.4.1",
		"@thatopen/ui-obc": "^2.4.1",
		"@vitejs/plugin-vue": "^5.2.3",
		"atomic-router": "^0.10.1",
		"atomic-router-react": "^0.10.0",
		"clsx": "^2.1.1",
		"date-fns": "^2.30.0",
		"effector": "^23.3.0",
		"effector-react": "^23.3.0",
		"motion": "^12.5.0",
		"openapi-fetch": "^0.13.5",
		"openapi-ff": "^0.1.3",
		"openapi-typescript": "^7.6.1",
		"patronum": "^2.3.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-dropzone": "^14.3.8",
		"react-helmet": "^6.1.0",
		"react-window": "^1.8.11",
		"tailwind-merge": "^3.0.2",
		"three": "^0.160.1",
		"vite-plugin-html": "^3.2.2",
		"web-ifc": "^0.0.66",
		"xlsx": "^0.18.5",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@eslint/js": "^9.17.0",
		"@types/node": "^22.13.10",
		"@types/react": "^18.3.18",
		"@types/react-dom": "^18.3.5",
		"@types/three": "^0.172.0",
		"@vitejs/plugin-react": "^4.3.4",
		"autoprefixer": "^10.4.21",
		"eslint": "^9.17.0",
		"eslint-plugin-react-hooks": "^5.0.0",
		"eslint-plugin-react-refresh": "^0.4.16",
		"globals": "^15.14.0",
		"postcss": "^8.5.3",
		"tailwindcss": "^4.0.15",
		"typed-openapi": "^0.10.1",
		"typescript": "~5.6.2",
		"typescript-eslint": "^8.18.2",
		"vite": "^6.0.5"
	}
}
